{
  "name": "@pedrovs3/react-router",
  "version": "1.4.6",
  "description": "This is a simple, lightweight, and flexible routing library for React applications. It provides a set of components and hooks to handle routing in your application, including support for dynamic routes with path parameters.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "scripts": {
    "test": "jest --watchAll --coverage",
    "build": "tsup src/index.ts --format cjs,esm --dts --minify --clean",
    "npm:link": "npm run build && npm link"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json",
    "CHANGELOG.md"
  ],
  "keywords": [
    "react",
    "router"
  ],
  "repository": "https://github.com/pedrovs3/react-router.git",
  "jest": {
    "testEnvironment": "jsdom"
  },
  "author": "pedrovs3",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "typescript": "^5.3.3",
    "tsup": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "eslint": "8.56.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-n": "16.0.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "^7.33.2"
  }
}
